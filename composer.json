{
    "name": "yushkevichv/pdf-cad-reader",
    "description": "Parse PDF CAD files for get index meta info and decoded stream vector data",
    "type": "library",
    "require": {
        "tecnickcom/tcpdf": "^6.2",
        "ext-zlib": "*",
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Yushkevichv\\PDFCadReader\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yushkevichv\\PDFCadReader\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Vitaliy Yushkevich",
            "email": "yushkevichv@gmail.com"
        }
    ],
    "minimum-stability": "dev"
}
